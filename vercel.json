{
  "buildCommand": "npm run build:vercel --prefix client",
  "framework": "angular",
  "installCommand": "npm update --global npm && npm ci && npm ci --prefix client",
  "outputDirectory": "client/dist/mean-authentication/browser",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-robots-tag",
          "value": "none"
        }
      ]
    }
  ]
}
